{%- set command_ip_map = {} -%}
{%- for aid, reason in xcon_list.items() -%}
    {%- if reason | string == 'xcon_required' -%}
        {# 1-A-1-7 to 1-A-1-7T-1 #}
        {%- set c_aid = aid[:-1] + 'T' + aid[-1] + '-1' -%}
        {%- set cmd = "RTRV-CRS:%s:%s-ODU2e#1,%s-ODU2e#2:CTAG::;" | format(TID,c_aid,c_aid) -%}
        {%- set dummy = command_ip_map.update({cmd: {}}) -%}
    {%- endif -%}
{%- endfor -%}
{{- command_ip_map -}}